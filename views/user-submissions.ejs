<% include partials/header.ejs %>

    <div class="container">
        <div class="row">
            <% submissions.forEach(function(sub){ %>
                <div class="col-lg-3 col-md-4 col-sm-6">
                    <img src="/<%= sub.sketch %>" alt="" style="width: 400px; height: 400px;">
                </div> 
            <% }) %>
        </div>
    </div>

    <div class="container">
        <div class="row">
           <% submissions.forEach(function(sub){ %>
                <div class="col-lg-3 col-md-4 col-sm-6">
                    <video width="400" height="400" controls>
                        <source src="/<%= sub.cover %>" type="video/mp4">
                    </video>
                </div> 
           <% }) %>
        </div>
    </div>    


<% include partials/footer.ejs %>
