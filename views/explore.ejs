<% include partials/header.ejs %>

<div class="main-container">
	<h1 id="artistheading">
		<%= data.routeType == 'submissions' ? 
		"Checkout Submissions For:" :
		"Artists/Bands On Tour" %>
	</h1>
	<div class="container">
		<div class="row text-center">
		<%	data.celeb.forEach(function(celeb){   %>
			<div class="col-lg-3 col-md-4 col-sm-6">
				<a href="<%= data.routeType == 'submissions' ? '/submissions/'+celeb._id : '/events/'+celeb._id%>">
					<div class="thumbnail">
						<img src="<%= celeb.image %>">
						<div class="caption" style="width: 100%">
							<h3><%= celeb.celebName %></h3>
						</div>
					</div>
				</a>
			</div>
		<% }) %>
		</div>
	</div>
</div>

<% include partials/footer.ejs %>